{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:vue/essential", "standard"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "semi": ["error", "always"],
    "comma-dangle": ["error", "always"]
  }
}
